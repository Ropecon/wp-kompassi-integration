#. Plugin Name of the plugin
#: kompassi-integration.php:111 kompassi-integration.php:125
msgid "Kompassi Integration"
msgstr "Kompassi-integraatio"

#. Description of the plugin
msgid "Integrate data from Kompassi to your WordPress site easily"
msgstr "Yhdistä tietoja Kompassista WordPress-sivustollesi helposti"

#. Author of the plugin
msgid "Pasi Lallinaho"
msgstr "Pasi Lallinaho"

#: kompassi-integration.php:51
msgid "Event Technical Name"
